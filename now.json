{
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "",
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": ""
      },
      "continue": true
    },
    {
      "src": "/api/([^/]+)",
      "dest": "/$1.js"
    }
  ]
}
